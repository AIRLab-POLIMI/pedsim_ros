<?xml version="1.0" encoding="UTF-8"?>
<scenario>
    <!--Obstacles-->
    <obstacle x1="0" y1="0" x2="20" y2="0"/>
    <obstacle x1="0" y1="0" x2="0" y2="20"/>
    <obstacle x1="0" y1="20" x2="20" y2="20"/>
    <obstacle x1="20" y1="20" x2="20" y2="0"/>

    <!--Waypoints (incl. WaitingQueues)-->
    <waypoint id="dd" x="2" y="18" r="3"/>
    <waypoint id="cc" x="15" y="15" r="3"/>
    <waypoint id="aa" x="5" y="5" r="3"/>
    <waypoint id="bb" x="18" y="2" r="3"/>

    <waypoint id="robot_goal" x="18" y="18" r="1"/>
    <waypoint id="robot_start" x="2" y="2" r="1"/>

    <!--Agents-->
    <agent x="3" y="3" n="1" dx="0" dy="0" type="2">
        <addwaypoint id="robot_start"/>
        <addwaypoint id="robot_goal"/>
    </agent>

    <!--AgentClusters-->
    <agent x="5" y="5" n="8" dx="3" dy="3" type="1">
        <addwaypoint id="aa"/>
        <addwaypoint id="bb"/>
        <addwaypoint id="cc"/>
        <addwaypoint id="dd"/>
    </agent>

    <agent x="15" y="15" n="8" dx="4" dy="4" type="0">
        <addwaypoint id="aa"/>
        <addwaypoint id="dd"/>
        <addwaypoint id="bb"/>
        <addwaypoint id="cc"/>
    </agent>
</scenario>
